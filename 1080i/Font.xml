<?xml version="1.0" encoding="UTF-8"?>
<fonts>

    <fontset id="Default" unicode="false">
        <include>Font_Default</include>
    </fontset>

    <fontset id="Inter (Experimental)" unicode="false">
        <include content="Font_Default">
            <param name="font_black">Inter-Bold.ttf</param>
            <param name="font_bold">Inter-Bold.ttf</param>
            <param name="font_regular">Inter-Regular.ttf</param>
            <param name="font_light">Inter-Light.ttf</param>
            <param name="plot_linespacing_head">1.1</param>
            <param name="plot_linespacing_midi">1.43</param>
            <param name="plot_linespacing_main">1.107</param>
            <param name="plot_linespacing_mini">1.205</param>
            <param name="plot_linespacing_tiny">1.07</param>
        </include>
    </fontset>

    <fontset id="Nunito (Experimental)" unicode="false">
        <include content="Font_Default">
            <param name="font_black">Nunito-Bold.ttf</param>
            <param name="font_bold">Nunito-Bold.ttf</param>
            <param name="font_regular">Nunito-Regular.ttf</param>
            <param name="font_light">Nunito-Light.ttf</param>
            <param name="plot_linespacing_head">0.982</param>
            <param name="plot_linespacing_midi">1.26</param>
            <param name="plot_linespacing_main">0.98</param>
            <param name="plot_linespacing_mini">1.07</param>
            <param name="plot_linespacing_tiny">0.96</param>
        </include>
    </fontset>

    <fontset id="NotoRoboto - Arabic / Hebrew" unicode="true">
        <include content="Font_Default">
            <param name="font_black">RobotoNotoCondensed-Bold.ttf</param>
            <param name="font_bold">RobotoNotoCondensed-Bold.ttf</param>
            <param name="font_regular">RobotoNotoCondensed-Regular.ttf</param>
            <param name="font_light">RobotoNotoCondensed-Light.ttf</param>
        </include>
    </fontset>

    <fontset id="CJK - Chinese / Japanese / Korean" unicode="true">
        <include content="Font_Default">
            <param name="font_black">resource://resource.font.robotocjksc/RobotoCondensedCJK-Bold.ttf</param>
            <param name="font_bold">resource://resource.font.robotocjksc/RobotoCondensedCJK-Bold.ttf</param>
            <param name="font_regular">resource://resource.font.robotocjksc/RobotoCondensedCJK-Regular.ttf</param>
            <param name="font_light">resource://resource.font.robotocjksc/RobotoCondensedCJK-Light.ttf</param>
        </include>
    </fontset>

</fonts>

